{
  "name": "gadget-house",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint:prettify": "prettier --check \"**/*.{ts,tsx,json,md,css,html,js,jsx}\"",
    "prettify": "prettier --write \"**/*.{ts,tsx,json,md,css,html,js,jsx}\"",
    "lint:css": "npx stylelint \"src/**/*.css\"",
    "lint:js": "npx eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint:ts": "tsc --noEmit",
    "lint": "npm run lint:css && npm run lint:js && npm run lint:ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^5.4.0",
    "@reduxjs/toolkit": "^2.2.7",
    "@types/lodash.debounce": "^4.0.9",
    "antd": "^5.19.4",
    "axios": "^1.7.7",
    "classnames": "^2.5.1",
    "dompurify": "^3.1.6",
    "framer-motion": "^11.5.4",
    "gadget-house": "file:",
    "lodash.debounce": "^4.0.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "react-loading-skeleton": "^3.5.0",
    "react-redux": "^9.1.2",
    "react-responsive": "^10.0.0",
    "react-router-dom": "^6.28.0",
    "react-toastify": "^10.0.5",
    "redux-logger": "^3.0.6",
    "reselect": "^5.1.1",
    "stylelint": "^16.8.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-order": "^6.0.4"
  },
  "devDependencies": {
    "@types/dompurify": "^3.0.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/redux-logger": "^3.0.13",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "typescript": "^5.5.3",
    "vite": "^5.3.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.6.1"
  },
  "lint-staged": {
    "**/*.{js,jsx.ts,tsx}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  }
